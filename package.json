{
  "name": "color-check",
  "description": "",
  "version": "0.0.0-Ask-SemVerCli",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/motleydev/color-check.git"
  },
  "author": {
    "name": "Jesse Martin",
    "email": "admin@jessedavidmartin.com",
    "url": "jessedavidmartin.com"
  },
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "build": "babel src -d dist",
    "post-build": "onchange 'dist/**.js' 'test/**.js' -- npm test",
    "watch": "npm run build -- -w & npm run post-build",
    "test": "standard && mocha --compilers js:babel-register",
    "prepublish": "npm run build && npm test",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [
    "WCAG",
    "WCAG2",
    "color",
    "wcag",
    "wcag2",
    "snook",
    "contrast",
    "legibility"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel": "*",
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-register": "6.16.3",
    "chai": "*",
    "cz-conventional-changelog": "1.2.0",
    "mocha": "*",
    "onchange": "1.x",
    "rimraf": "2.5.4",
    "semantic-release": "^4.3.5",
    "standard": "*"
  },
  "standard": {
    "ignore": [
      "coverage/**",
      "dist/**",
      "node_modules/**"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
